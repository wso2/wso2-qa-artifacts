        <!-- Campaign Finance API -->

        <!-- Candidates -->

        <resource path="elections/us/{version}/finances/{campaign-cycle}/candidates/search.{response-format}">
            <param name="version" type="xsd:string" style="query" required="true" default="v3">
                <doc apigee:url="http://developer.nytimes.com/docs/campaign_finance_api/#h3-common-params">
                    <![CDATA[The API version. The current version is v3. Support for version 2 ended in February 2011. (Version 1 is for internal use only.)]]>
                </doc>
            </param>

            <param name="campaign-cycle" type="xsd:string" style="query" required="true">
                <doc apigee:url="http://developer.nytimes.com/docs/campaign_finance_api/#h3-common-params">
                    <![CDATA[An even numbered year representing the campaign cycle. Presidential data: 2008–present. Congressional data: 2000–present. Format: YYYY]]>
                </doc>
            </param>

            <param name="response-format" type="xsd:string" style="query" required="false">
                <doc apigee:url="http://developer.nytimes.com/docs/campaign_finance_api/#h3-common-params">
                    <![CDATA[Sets the representation data format. To return JSON, add the .json extension to your request URI. XML is the default format; you do not need to add the .xml extension to get an XML response. Accepted values: json (implicit default), xml]]>
                </doc>
            </param>

            <method id="candidate_search" name="GET" apigee:displayName="elections/us/{version}/finances/{campaign-cycle}/candidates/search.{response-format}">
                <apigee:tags>
                    <apigee:tag primary="true">Campaign Finance API</apigee:tag>
                    <apigee:tag>Candidates</apigee:tag>
                </apigee:tags>

                <apigee:authentication required="false"/>

                <apigee:example url="elections/us/v3/finances/{campaign-cycle}/candidates/search"/>

                <doc title="candidate_search" apigee:url="http://developer.nytimes.com/docs/campaign_finance_api/#h3-candidate-search">
                    <![CDATA[Searches for Congressional and presidential candidates by last name.]]>
                </doc>

                <request>
                    <param name="lname" type="xsd:string" style="query" required="true">
                        <doc apigee:url="http://developer.nytimes.com/docs/campaign_finance_api/#h3-candidate-search">
                            <![CDATA[The candidate last name or partial last name. Examples: Greene (retrieves Greene), Robin (retrieves both Robin and Robinson)]]>
                        </doc>
                    </param>

                    <param name="callback" type="xsd:string" style="query" required="false">
                        <doc apigee:url="http://developer.nytimes.com/docs/campaign_finance_api/#h3-common-params">
                            <![CDATA[JSONP callback function. As of February 2011, the Campaign Finance API supports JSONP requests.]]>
                        </doc>
