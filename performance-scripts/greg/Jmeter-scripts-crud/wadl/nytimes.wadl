                    <![CDATA[Returns movie reviews by keyword.]]>
                </doc>

                <request>
                    <!-- search parameters -->
                    <param name="api-key" type="xsd:string" style="query" required="true" default="887a8ea540f1b1c44a32584e84ee9383:9:64325990">
                        <doc apigee:url="http://developer.nytimes.com/docs/reference/keys">
