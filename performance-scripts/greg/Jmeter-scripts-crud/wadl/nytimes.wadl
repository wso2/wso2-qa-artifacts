                    <![CDATA[Sets the representation data format. To return XML or serialized PHP, add the appropriate extension to your request URI. JSON is the default format; you do not need to add the .json extension to get a JSON response. Accepted values: json (implicit default), xml, sphp]]>
                </doc>
            </param>

            <method id="get_best_seller_history" name="GET" apigee:displayName="books/{version}/lists/best-sellers/history">
                <apigee:tags>
                    <apigee:tag primary="true">Best Sellers API</apigee:tag>
                </apigee:tags>

                <apigee:authentication required="false"/>

                <apigee:example url="books/v2/lists/best-sellers/history"/>

                <doc title="get_best_seller_history" apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#h3-history">
                    <![CDATA[Returns the history of a best seller (its rank and number of weeks on the various Times best-seller lists).]]>
                </doc>

                <request>

                    <param name="api-key" type="xsd:string" style="query" required="true" default="d28cb3126eae332c6279f9dedf4bb830:10:64325990">
                        <doc apigee:url="http://developer.nytimes.com/docs/reference/keys">
                            <![CDATA[API Key for Best Sellers API.]]>
                        </doc>
                    </param>

                    <param name="age-group" type="xsd:string" style="query" required="false">
                        <doc apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#sortsearchbooks">
                            <![CDATA[Search parameter; at least one is required. See API docs for more info.]]>
                        </doc>
                    </param>

                    <param name="author" type="xsd:string" style="query" required="false">
                        <doc apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#sortsearchbooks">
                            <![CDATA[Search parameter; at least one is required. See API docs for more info.]]>
                        </doc>
                    </param>

                    <param name="contributor" type="xsd:string" style="query" required="false">
                        <doc apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#sortsearchbooks">
                            <![CDATA[Search parameter; at least one is required. See API docs for more info.]]>
                        </doc>
                    </param>

                    <param name="isbn" type="xsd:string" style="query" required="false">
                        <doc apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#sortsearchbooks">
                            <![CDATA[Search parameter; at least one is required. See API docs for more info.]]>
                        </doc>
                    </param>

                    <param name="price" type="xsd:string" style="query" required="false">
                        <doc apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#sortsearchbooks">
                            <![CDATA[Search parameter; at least one is required. See API docs for more info.]]>
                        </doc>
                    </param>

                    <param name="publisher" type="xsd:string" style="query" required="false">
                        <doc apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#sortsearchbooks">
                            <![CDATA[Search parameter; at least one is required. See API docs for more info.]]>
                        </doc>
                    </param>

                    <param name="title" type="xsd:string" style="query" required="false">
                        <doc apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#sortsearchbooks">
                            <![CDATA[Search parameter; at least one is required. See API docs for more info.]]>
                        </doc>
                    </param>

                    <!-- optional parameters -->

                    <param name="sort-by" type="xsd:string" style="query" required="false" default="rank">
                        <doc apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#h3-common-params">
                            <![CDATA[For details on the available sort options, see API docs. If you do not specify a sort-by value, the results will be sorted by title (exception: if you are searching by ISBN, the results will be sorted by ISBN). Accepted values: age-group, author, contributor, isbn, price, publisher, title]]>
                        </doc>
                    </param>

                    <param name="sort-order" type="xsd:string" style="query" required="false" default="ASC">
                        <doc apigee:url="http://developer.nytimes.com/docs/best_sellers_api/#h3-common-params">
                            <![CDATA[Sets the sort order of the result set. The default is ASC (ascending). The sort-order parameter is used with the sort-by parameter — for details, see each request type. Accepted values: ASC (ascending), DESC (descending)]]>
                        </doc>
                    </param>
                </request>

                <response>
                    <representation mediaType="application/json"/>
                    <representation mediaType="application/xml"/>
                    <representation mediaType="application/vnd.php.serialized"/>
                </response>
            </method>
