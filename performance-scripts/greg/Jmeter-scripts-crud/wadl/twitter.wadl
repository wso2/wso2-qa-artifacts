        <resource path="geo/nearby_places.{format}">
            <param name="format" type="xsd:string" style="template" required="true" default="json">
                <option value="json" mediaType="application/json"/>
            </param>
            <param name="ip" required="false" type="xsd:string" style="query"  default=""/>
            <param name="long" required="false" type="xsd:string" style="query"  default=""/>
            <param name="max_results" required="false" type="xsd:string" style="query"  default=""/>
